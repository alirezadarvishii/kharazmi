            <% if(comment.user !== null) { %> <%= comment.user.fullname %> <% if(comment.user.role === "admin") { %>
            <span class="badge bg-danger">مدیریت</span>
            <% } else if(comment.user.role === "teacher") { %>
            <span class="badge bg-warning">معلم</span>
            <% } else if(comment.user.role === "user") { %>
            <span class="badge bg-success">کاربر عادی</span>
            <% } %> <% } else { %> کاربر دلیت اکانت شده
            <span class="badge bg-gray">مشخص نیست</span>
            <% } %>
