<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="./bootstrap/css/bootstrap.rtl.min.css" />
    <title><%= title %></title>
    <script defer src="./js/app-ffbfe456f36267f13b08.js"></script>
    <link href="./styles/app-2940de3c8d4c5a1e20a6.css" rel="stylesheet" />
  </head>
  <body>
    <%- include("./includes/nav.ejs") %> <%- include("./includes/small-header.ejs") %>

    <!-- Masters -->
    <section class="masters-area my-5">
      <div class="container-lg row mx-auto">
        <% departman.forEach(departman => { %>
        <div class="col-12 col-sm-6 col-md-4 col-xl-3 position-relative my-5">
          <a href="#">
            <div class="card h-100 border-0 position-relative">
              <div class="master-img position-relative overflow-hidden">
                <img src="http://localhost:3000/users/<%= departman.profileImg %>" class="card-img-top p-2 rounded-8 fit-cover" alt="" />
              </div>
              <div class="card-body position-absolute bg-white rounded-3 shadow-sm">
                <h6 class="card-title text-center font-bold text-primary"><%= departman.fullname %></h6>
                <p class="text-secondary text-center font-light mb-0">مدرس دروس تخصصی</p>
              </div>
            </div>
          </a>
        </div>
        <% }) %>
        <!-- Pagination -->
        <nav aria-label="Page navigation">
          <ul class="pagination mt-5 justify-content-center">
            <li class="page-item">
              <a class="page-link" href="#" aria-label="Previous">
                <span aria-hidden="true">&laquo;</span>
              </a>
            </li>
            <li class="page-item active"><a class="page-link" href="#">1</a></li>
            <li class="page-item"><a class="page-link" href="#">2</a></li>
            <li class="page-item"><a class="page-link" href="#">3</a></li>
            <li class="page-item">
              <a class="page-link" href="#" aria-label="Next">
                <span aria-hidden="true">&raquo;</span>
              </a>
            </li>
          </ul>
        </nav>
        <!--/ Pagination -->
      </div>
    </section>
    <!--/ Masters -->

    <%- include("./includes/footer.ejs") %>
  </body>
  <script src="./bootstrap/js/bootstrap.min.js"></script>
</html>

<!-- hello world -->
