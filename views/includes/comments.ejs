        <% if(comment.user !== null) { %>
        <div class="shadow rounded-pill overflow-hidden user-profile">
          <a href="/user/profile/<%= comment.user.role %>/<%= comment.user._id %>">
            <img src="http://localhost:3000/users/<%= comment.user.profileImg %>" class="rounded-pill p-1 fit-cover" alt="" />
          </a>
        </div>
        <% } %>
